{
  "name": "server",
  "version": "1.2.0",
  "description": "[MNA] bal server",
  "repository": "https://github.com/mission-apprentissage/bal.git",
  "private": true,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup src/index.ts migrations --watch . --watch ../shared --onSuccess 'yarn run start'",
    "build": "tsup src/index.ts migrations",
    "typecheck": "tsc",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "cli": "ts-node -T src/modules/data/cli.ts ",
    "cli-docker": "docker exec -it bal_server yarn cli",
    "test": "yarn test:cmd 'tests/**/*.test.ts'",
    "test:coverage": "c8 --reporter lcov --reporter text yarn test",
    "test:cmd": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test ts-mocha -n loader=ts-node/esm"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "axios": "0.24.0",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "cli": "^1.0.1",
    "commander": "^10.0.0",
    "csv-parse": "^5.3.5",
    "dotenv": "^16.0.3",
    "env-var": "7.1.1",
    "fastify": "^4.12.0",
    "json-schema-to-ts": "^2.7.2",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.21",
    "luxon": "2.3.0",
    "migrate-mongo": "9.0.0",
    "mongodb": "4.11.0",
    "omit-deep": "0.3.0",
    "rate-limiter-flexible": "2.3.6",
    "shared": "*",
    "stream-transform": "^3.2.2",
    "ts-node": "^10.9.1",
    "uuid": "9.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/bunyan-prettystream": "^0.1.32",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/mocha": "^10.0.1",
    "@types/omit-deep": "^0.3.0",
    "c8": "^7.13.0",
    "chalk": "4.1.2",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^8.12.1",
    "nodemon": "^2.0.20",
    "ts-mocha": "^10.0.0",
    "tsup": "^6.5.0",
    "typescript": "4.9.4"
  }
}
