    "@fastify/type-provider-json-schema-to-ts": "2.2.2",
    "@hapi/boom": "10.0.1",
    "axios": "0.24.0",
    "axios-cache-interceptor": "1.0.1",
    "bunyan": "1.8.15",
    "bunyan-prettystream": "0.1.3",
    "checksum-stream": "1.0.3",
    "clamscan": "2.1.2",
    "commander": "10.0.0",
    "company-email-validator": "1.0.9",
