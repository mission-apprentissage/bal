{
  "name": "server",
  "version": "1.33.2-0",
  "description": "[MNA] bal server",
  "repository": "https://github.com/mission-apprentissage/bal.git",
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js start",
    "dev": "tsup src/index.ts src/db/migrations --watch ./src --watch ../shared/src --onSuccess 'yarn run start'",
    "build": "tsup src/index.ts src/db/migrations",
    "typecheck": "tsc --noEmit",
    "migrations:create": "yarn cli migrations:create",
    "cli": "node dist/index.js",
    "cli-docker": "docker exec -it bal_server yarn cli",
    "test": "vitest -c ./vitest.config.ts --run",
    "test:coverage": "yarn test --coverage",
    "test:watch": "vitest -c ./vitest.config.ts --watch"
  },
  "dependencies": {
