    "axios-cache-interceptor": "1.0.1",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "checksum-stream": "1.0.3",
    "clamscan": "2.1.2",
    "cli": "1.0.1",
    "commander": "10.0.0",
    "company-email-validator": "1.0.9",
    "convert-csv-to-json": "2.0.0",
    "csv-parse": "5.3.5",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "ejs": "3.1.9",
    "env-var": "7.1.1",
    "fastify": "4.12.0",
    "json2csv": "6.0.0-alpha.2",
