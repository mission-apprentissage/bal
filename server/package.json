{
  "name": "server",
  "version": "1.30.23",
  "description": "[MNA] bal server",
  "repository": "https://github.com/mission-apprentissage/bal.git",
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup src/index.ts src/modules/data/cli.ts migrations --watch . --watch ../shared --onSuccess 'yarn run start'",
    "build": "tsup src/index.ts src/modules/data/cli.ts migrations",
    "typecheck": "tsc",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "cli": "node dist/modules/data/cli.js",
    "cli-docker": "docker exec -it bal_server yarn cli",
    "test": "yarn test:cmd tests/**/*.test.ts",
    "test:coverage": "c8 --reporter lcov --reporter text yarn test",
    "test:cmd": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test ts-mocha -r tsx"
  },
  "dependencies": {
    "@fastify/auth": "4.2.0",
    "@fastify/cookie": "8.3.0",
